    "param converter"
  ],
  "require": {
    "php": ">=7.0",
    "doctrine/annotations": "~1.0",
    "symfony/property-access": "^3.4|^4.4|^5.0",
    "sensio/framework-extra-bundle": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">5.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "Metglobal\\Compass\\DTO\\": "src/",
      "Metglobal\\Compass\\DTO\\Tests\\": "tests/"
    }
  },
